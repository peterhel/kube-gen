upstream <%= object.metadata.name %> {
  <% pods.items.forEach(pod => { %>server <%= pod.spec.nodeName %>:<%= ports.web && ports.web.nodePort || object.spec.ports[0].nodePort %>;
  <% }) %>
}

server {
    server_name <%= object.metadata.name %>.example.com;
    location / {
        proxy_pass http://<%= object.metadata.name %>;
    }
}
